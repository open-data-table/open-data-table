{
  "name": "open-data-table",
  "version": "0.0.2",
  "description": "A Material Design data table web component.",
  "author": "Paul H mason",
  "license": "MIT",
  "main": "",
  "module": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-data-table/open-data-table.git"
  },
  "scripts": {
    "start": "es-dev-server --app-index demo/index.html --node-resolve --open",
    "start:watch": "es-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:current": "karma start --grep test/open-data-table.test.js --coverage",
    "pub": "npm publish src",
    "pub:test": "npm publish src --dry-run",
    "build": "rimraf dist && rollup -c rollup.config.js"
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2"
  },
  "devDependencies": {
    "es-dev-server": "^1.23.0",
    "@open-wc/testing-karma": "^3.0.0",
    "deepmerge": "^3.2.0",
    "@open-wc/testing": "^2.0.0",
    "@open-wc/building-rollup": "^1.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^2.9.1"
  }
}
